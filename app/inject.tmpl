                <md-menu md-position-mode="target-right target">
                  <md-button aria-label="More Otions" class="md-icon-button sae-action-button" ng-click="$mdMenu.open($event)">
                    <md-icon md-font-icon="fa fa-ellipsis-v"></md-icon>
                  </md-button>
                  <md-menu-content width="2">
                    <md-menu-item>
                      <md-button class="" ng-click="appCtrl.formGrid.cloneForm(summaryRow.annoton)">
                        <md-icon md-font-icon="fa fa-clone"></md-icon> Clone
                      </md-button>
                    </md-menu-item>
                    <md-menu-item>
                      <md-button class="" ng-click="appCtrl.graph.deleteAnnoton(summaryRow.annoton, $event)">
                        <md-icon md-font-icon="fa fa-trash"></md-icon> Delete
                      </md-button>
                    </md-menu-item>
                  </md-menu-content>
                </md-menu>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div>
        <h2>
          CC Only Annotations
        </h2>
      </div>
