    <br>
    <div ng-if="tvc.graph.loggedIn" class="row">
      <div class="col-xs-8">
        <div ng-if="tvc.saveRowEnabled(patternForm).length > 0" class="well well-sm" style="margin:0;padding:3px;">
          <ul>
            <li ng-repeat="reason in tvc.saveRowEnabled(patternForm)" class="error-reason">
              {{reason}}
            </li>
          </ul>
        </div>
      </div>

      <div class="col-xs-3">
        <md-button type="button" ng-disabled="tvc.saveRowEnabled(patternForm).length > 0" ng-click="tvc.saveRow()" class="btn btn-primary btn-sm center-block btn-block"
          style="margin-top:3px;">
          Save Annoton
          </button>
      </div>
    </div>

    <br>

    <div class="row">
      <div class="col-xs-12">
        <div id="grid1" ui-grid="tvc.gridOptions" ui-grid-edit ui-grid-cellNav ui-grid-resize-columns ui-grid-auto-resize ui-grid-tree-view
          class="grid" style="margin:0;padding:0;height:80%;width:100%;">
        </div>
      </div>
    </div>
  </div>

  <script type="text/ng-template" id="uigridActionHeader">
  <!--     <button
        type="button"
        title="Add Association"
        class="btn btn-primary btn-xs xbtn-compact"
        xng-click="grid.appScope.tvc.addRow()">
        <i class="fa fa-plus fa-fw"></i>
      </button>
   -->
    </script>

  <script type="text/ng-template" id="uigridActionCell">
      <div
        class="ui-grid-cell-contents"
        title="TOOLTIP">
        <div
          ng-if="grid.appScope.tvc.graph.loggedIn && row.treeLevel === 0"
          class="text-center action-row">
          <button
            type="button"
            title="Edit Annoton"
            class="btn btn-default btn-xs btn-compact"
            ng-click="grid.appScope.tvc.editRow(row.entity)">
            <i class="fa fa-pencil fa-fw"></i>
          </button>
        </div>
        <div
          ng-if="grid.appScope.tvc.graph.loggedIn && row.treeLevel === 0"
          class="text-center action-row">
          <button
            type="button"
            title="Delete Annoton"
            class="btn btn-default btn-xs btn-compact"
            ng-click="grid.appScope.tvc.deleteRow(row.entity)">
            <i class="fa fa-trash-o fa-fw"></i>
          </button>
        </div>
      </div>
    </script>

  <script type="text/ng-template" id="customTemplate.html">
      <a>
        <span style="display:inline-block;width:200px;font-size:0.9em;font-style:italic;" ng-bind-html="match.model.id | uibTypeaheadHighlight:query"></span>
        <span ng-bind-html="match.model.label | uibTypeaheadHighlight:query"></span>
      </a>
    </script>

  <script type="text/ng-template" id="rowTemplate.html">
       <div
        ng-repeat="(colRenderIndex, col) in colContainer.renderedColumns track by col.uid"
        ui-grid-one-bind-id-grid="rowRenderIndex + '-' + col.uid + '-cell'"
        class="ui-grid-cell"
        ng-class="{'ui-grid-row-header-cell': col.isRowHeader, 'ui-grid-row-summary': row.treeLevel === 0 }"
        role="{{col.isRowHeader ? 'rowheader' : 'gridcell'}}"
        ui-grid-cell>
      </div>
    </script>

  <script type="text/ng-template" id="cellTemplate.html">
      <div
        style="white-space:pre-line !important;"
        class="ui-grid-cell-contents"
        title="TOOLTIP">{{COL_FIELD CUSTOM_FILTERS}}</div>
    </script>
</div>


