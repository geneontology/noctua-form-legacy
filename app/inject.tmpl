                <md-button ng-if="!summaryRow.annoton.parser.clean" class="" ng-click="appCtrl.dialogService.openAnnotonErrorsDialog($event, summaryRow.annoton, summaryRow.annoton.parser.errors)">
                  <span class="text-danger">
                    {{summaryRow.annoton.parser.errors.length}} error(s)
                  </span>
                </md-button>
                <span flex></span>
                <md-button ng-if="true" class="ae-action-button" ng-click="appCtrl.dialogService.openViewSummaryDialog($event, summaryRow)">
